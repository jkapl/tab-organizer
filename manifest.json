{
  "name" : "Tab Organizer",
  "version": "1.0",
  "description": "Tab management tool",
  "background" : {
    "scripts": ["index.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions" : [
    "tabs", "storage"
  ],
  "manifest_version": 2
}